{
  "name": "plugin-web-update-notification",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@7.2.1",
  "description": "Detect web page updates and notify",
  "author": "Utopia",
  "license": "MIT",
  "homepage": "https://github.com/GreatAuk/vite-plugin-web-update-notification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreatAuk/vite-plugin-web-update-notification"
  },
  "bugs": "https://github.com/GreatAuk/vite-plugin-web-update-notification/issues",
  "keywords": [
    "plugin-web-update-notification"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm --filter=./packages/** build",
    "dev:vite": "pnpm --filter=vite-plugin-web-update-notification dev",
    "dev:umi": "pnpm --filter=umijs-plugin-web-update-notification dev",
    "example:vue-vite": "pnpm --filter=vue-vite-example preview",
    "example:react-vite": "pnpm --filter=react-vite-example preview",
    "example:svelte-vite": "pnpm --filter svelte-vite-example preview",
    "example:react-umi": "pnpm --filter react-umi-example preview",
    "lint": "eslint .",
    "synchronous-doc": "esno scripts/copyFile.ts",
    "prepublishOnly": "nr build",
    "release": "bumpp package.json packages/**/package.json",
    "test:e2e": "pnpm --filter vue-vite-example test:e2e",
    "test:unit": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  }
}
