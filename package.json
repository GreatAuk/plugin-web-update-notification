{
  "name": "vite-plugin-web-update-notification",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Detect web page updates and notify",
  "author": "Utopia",
  "license": "MIT",
  "homepage": "https://github.com/GreatAuk/vite-plugin-web-update-notification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreatAuk/vite-plugin-web-update-notification"
  },
  "bugs": "https://github.com/GreatAuk/vite-plugin-web-update-notification/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-web-update-notification"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --ignore-watch example",
    "example:vue": "pnpm --filter vue-example preview",
    "example:react": "pnpm --filter react-example preview",
    "example:svelte": "pnpm --filter svelte-example preview",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --registry=https://registry.npmjs.org/",
    "start": "esno src/index.ts",
    "test:e2e": "pnpm --filter vue-example test:e2e",
    "test:e2e:ci": "pnpm --filter vue-example test:e2e:ci",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  }
}
